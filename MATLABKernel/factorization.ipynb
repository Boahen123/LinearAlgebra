    "% Create a matrix\n",
    "A = [2 4 -1 5 -2; -4 -5 3 -8 1; 2 -5 -4 1 8; -6 0 7 -3 1]\n",
    "\n",
    "% Perform LU factorization\n",
    "[L,U] = lu(A)\n",
    "\n",
    "% Convert the matrices to rational numbers\n",
    "L_rats = rats(L)\n",
    "U_rats = rats(U)\n"
   ]
  }
