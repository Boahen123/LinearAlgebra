   "source": [
    "% Create a matrix\n",
    "A = [2 4 6; 1 7 5; 3 8 1];\n",
    "\n",
    "% Perform LU factorization\n",
    "[L,U,P] = lu(A)\n",
    "\n",
    "% Convert the matrices to rational numbers\n",
    "L_rats = rats(L)\n",
    "U_rats = rats(U)\n",
    "P_rats = rats(P)"
